{
  "scripts": {
    "cucumber": "cucumber-js --require tests/e2e/cucumber.conf.js --format node_modules/cucumber-pretty --require tests/e2e/step-definitions --require-module babel-core/register tests/e2e/features",
    "test:chrome": "browser=chrome npm run cucumber",
    "test:firefox": "browser=firefox npm run cucumber",
    "json-server": "json-server --routes tests/e2e/routes.json --port $PORT --host $HOST tests/e2e/db.json --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "cucumber": "^5.1.0",
    "cucumber-pretty": "^1.5.0",
    "json-server": "^0.15.1",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "string": "^3.3.3"
  }
}
